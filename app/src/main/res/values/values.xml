<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="fab_margin">16dp</dimen>
    <dimen name="toolbar_height">50dp</dimen>
    <declare-styleable name="RoundImageView">
        <attr name="radius" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="ColorPickerView">
        <attr name="indicatorColor" format="color" /><!--指示点颜色-->
        <attr name="indicatorEnable" format="boolean" /><!--是否启用指示点-->
        <attr name="blackEnable" format="boolean" />
        <attr name="orientation" format="integer">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
    <dimen name="popupmenu_width">200dp</dimen>
    <dimen name="text_title_size">18sp</dimen>
    <dimen name="edittext_size">16sp</dimen>
</resources>